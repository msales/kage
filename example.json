{
  "log": "file",
  "log-file": "./kage.log",
  "log-level": "debug",
  "kafka": {
    "brokers": [
      "127.0.0.1:9092"
    ],
    "ignore": {
      "topics": [],
      "groups": []
    }
  },
  "reporters": [
    "influx",
    "stdout"
  ],
  "influx": {
    "dsn": "http://127.0.0.1:8086/metrics",
    "metric": "kafka",
    "tags": {
      "cluster": "local",
      "environment": "local"
    }
  },
  "server": {
    "address": "127.0.0.1:8888"
  }
}